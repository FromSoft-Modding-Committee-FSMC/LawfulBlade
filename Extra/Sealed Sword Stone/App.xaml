<Application x:Class="Sealed_Sword_Stone.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Sealed_Sword_Stone"
             Startup="OnApplicationStartup"
             StartupUri="Window/MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="960-540-Background" UriSource="LAUNCHER/960-540-Background.png"/>
            <BitmapImage x:Key="128-24-PlayButton" UriSource="LAUNCHER/128-24-PlayButton.png"/>
            <BitmapImage x:Key="128-24-SettingsButton" UriSource="LAUNCHER/128-24-SettingsButton.png"/>
            <BitmapImage x:Key="128-24-ExitButton" UriSource="LAUNCHER/128-24-ExitButton.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
